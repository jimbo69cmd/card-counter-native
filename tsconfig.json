{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "moduleResolution": "bundler",
    "customConditions": [
      "import",
      "require",
      "react-native"
    ]
  },
  "include": [
    "**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
